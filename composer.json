{
    "name": "vinkla/algolia",
    "description": "An Algolia bridge for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "algolia", "search", "api"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^1.25",
        "graham-campbell/manager": "^3.0",
        "illuminate/contracts": "5.5.*",
        "illuminate/support": "5.5.*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.0",
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Algolia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Algolia\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.8-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Algolia\\AlgoliaServiceProvider"
            ],
            "aliases": {
                "Algolia": "Vinkla\\Algolia\\Facades\\Algolia"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
